{
    "project_name": "CKA experiemnt",
    "arch": {
        "conv_dim": 16,
        "latent_dim": 4
    },
    "seed": 123,
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 10,
        "verbosity": 2,
        "monitor": "val_loss",
        "early_stop": 10,
        "tensorboard": true
    },
    "data_loader": {
        "type": "MnistDataLoader",
        "args": {
            "data_dir": "data/",
            "batch_size": 256,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "metrics": [
        "accuracy",
        "top_k_acc"
    ],
    "run_id": "test"
}